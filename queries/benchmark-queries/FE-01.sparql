# List all the evidence (supporting or challenging) and its type for claim X?
## this example uses claim-71 as claim X

SELECT DISTINCT xsd:string(?source) as ?evidence_source ?modality ?type
FROM <inferred-graph-oa-mp.xml>
WHERE 
{
      {?evidence mp:supports poc:ddi-spl-annotation-claim-71} UNION {?evidence mp:challenges poc:ddi-spl-annotation-claim-71} .
      ?evidence rdf:type ?type.  
      ?evidence ?modality poc:ddi-spl-annotation-claim-71.

      ?anItem oa:hasBody ?evidence;
	     oa:hasTarget ?aURN.

      ?aURN oa:hasSource ?source.

      FILTER regex ( ?type, dikbEvidence: )
}
