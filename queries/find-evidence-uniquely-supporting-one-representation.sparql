#### List all the evidence items that support exactly one representation which it is the only piece of evidence

SELECT ?representation ?evidence
FROM <http://purl.org/mp/initial-graph-oa-mp>
WHERE
{
     ?representation rdfs:subClassOf mp:Representation.

     ?evidence mp:supports ?representation.  
          
} HAVING (COUNT(?evidence) = 1) && (COUNT(?representation) = 1)
