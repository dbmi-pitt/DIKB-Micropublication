# How many pharmacokinetic studies provide data on the changes of a PK parameter in the case of a drug 
# interaction? 

SELECT count(?e) as ?num_pk_studies
FROM <inferred-graph-oa-mp.xml>
WHERE 
{
    {?e mp:supports ?c} UNION {?e mp:challenges ?c}.
     ?e dikbD2R:increases_auc ?v;
	rdf:type ?type.
     FILTER regex( ?type, dikbEvidence:EV_PK_DDI )    
}
