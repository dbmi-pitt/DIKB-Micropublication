#### List all the assertions that are challenged by exactly type of evidence

SELECT ?assertion
FROM <inferred-graph-oa-mp.xml>
WHERE 
{
      ?assertion rdf:type mp:Claim.
      ?evidence mp:challenges ?assertion;
          rdf:type ?type.
          FILTER regex(?type, dikbEvidence:)
} 
HAVING(count(DISTINCT ?type) = 1)
