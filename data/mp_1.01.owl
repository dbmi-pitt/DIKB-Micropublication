#################################################################
#
#    Object Properties
#
#################################################################


###  http://purl.org/mp/arguedBy

:arguedBy rdf:type owl:ObjectProperty ;
          
          rdfs:domain :Claim ;
          
          rdfs:range :Micropublication ;
          
          owl:inverseOf :argues ;
          
          rdfs:subPropertyOf :supports .



###  http://purl.org/mp/argues

:argues rdf:type owl:FunctionalProperty ,
                 owl:ObjectProperty ;
        
        rdfs:range :Claim ;
        
        rdfs:domain :Micropublication ;
        
        owl:equivalentProperty :hasClaim ;
        
        rdfs:subPropertyOf :supportedBy .



###  http://purl.org/mp/assertedBy

:assertedBy rdf:type owl:ObjectProperty ;
            
            owl:propertyDisjointWith :quotedBy ;
            
            rdfs:subPropertyOf owl:topObjectProperty .



###  http://purl.org/mp/asserts

:asserts rdf:type owl:ObjectProperty ;
         
         owl:inverseOf :assertedBy ;
         
         owl:propertyDisjointWith :quotes ;
         
         rdfs:subPropertyOf owl:topObjectProperty .



###  http://purl.org/mp/challengedBy

:challengedBy rdf:type owl:ObjectProperty ;
              
              owl:inverseOf :challenges ;
              
              rdfs:subPropertyOf owl:topObjectProperty .



###  http://purl.org/mp/challenges

:challenges rdf:type owl:ObjectProperty ;
            
            rdfs:comment """While 'supports' is transitive, 'challenges' is not.  The analogue to support transitivity is \"challengesSupportFor', which is the superproperty of a property chain on challenges and supports. 

If C1 supportedBy S1, S1 supportedBy D1, D1 supportedBy M1, M1 challengedBy C2; then C1 supportedBy M1 through support transitivity; and C2 challengesSupportFor C1 through its property chain. 

This corresponds to \"challenging the basis of an argument\", or \"undercutting an argument\".  

If I challenge the method by which you obtained some data, which support an interpretation on which you base your claim...then ultimately I am challenging your claim.""" ;
            
            rdfs:range :Representation ;
            
            rdfs:domain :Representation ;
            
            rdfs:subPropertyOf owl:topObjectProperty .



###  http://purl.org/mp/challengesSupportFor

:challengesSupportFor rdf:type owl:ObjectProperty ;
                      
                      rdfs:subPropertyOf owl:topObjectProperty ;
                      
                      owl:propertyChainAxiom ( :challenges
                                               :supports
                                             ) .



###  http://purl.org/mp/claimFor

:claimFor rdf:type owl:ObjectProperty ;
          
          owl:inverseOf :argues ;
          
          rdfs:subPropertyOf :supports .



###  http://purl.org/mp/derivedFrom

:derivedFrom rdf:type owl:ObjectProperty ;
             
             rdfs:range :Artifact ;
             
             rdfs:domain :Artifact ;
             
             owl:inverseOf :hasDerivation .



###  http://purl.org/mp/hasClaim

:hasClaim rdf:type owl:FunctionalProperty ,
                   owl:ObjectProperty ;
          
          rdfs:subPropertyOf :supportedBy .



###  http://purl.org/mp/hasDerivation

:hasDerivation rdf:type owl:ObjectProperty .



###  http://purl.org/mp/hasHolotype

:hasHolotype rdf:type owl:FunctionalProperty ,
                      owl:ObjectProperty ;
             
             rdfs:domain :Statement ;
             
             rdfs:range :Statement ;
             
             owl:inverseOf :holotypeFor .



###  http://purl.org/mp/hasProvenance

:hasProvenance rdf:type owl:FunctionalProperty ,
                        owl:ObjectProperty ;
               
               rdfs:domain :Artifact ;
               
               rdfs:range :Provenance ;
               
               owl:inverseOf :provenanceFor ;
               
               rdfs:subPropertyOf :supportedBy .



###  http://purl.org/mp/hasSupportGraph

:hasSupportGraph rdf:type owl:FunctionalProperty ,
                          owl:ObjectProperty ;
                 
                 rdfs:domain :Micropublication ;
                 
                 rdfs:range :SupportGraph ;
                 
                 rdfs:subPropertyOf :supportedBy .



###  http://purl.org/mp/holotypeFor

:holotypeFor rdf:type owl:InverseFunctionalProperty ,
                      owl:ObjectProperty ;
             
             rdfs:range :Statement ;
             
             rdfs:domain :Statement .



###  http://purl.org/mp/homologousTo

:homologousTo rdf:type owl:ObjectProperty ,
                       owl:ReflexiveProperty ,
                       owl:SymmetricProperty ;
              
              rdfs:range :Statement ;
              
              rdfs:domain :Statement ;
              
              owl:propertyChainAxiom ( :holotypeFor
                                     ) .



###  http://purl.org/mp/provenanceFor

:provenanceFor rdf:type owl:ObjectProperty ;
               
               rdfs:range :Artifact ;
               
               rdfs:domain :Provenance ;
               
               rdfs:subPropertyOf :supports .



###  http://purl.org/mp/qualifiedBy

:qualifiedBy rdf:type owl:ObjectProperty ;
             
             rdfs:range :Qualifier ;
             
             rdfs:domain :Statement .



###  http://purl.org/mp/qualifies

:qualifies rdf:type owl:ObjectProperty ;
           
           rdfs:domain :Qualifier ;
           
           rdfs:range :Statement ;
           
           owl:inverseOf :qualifiedBy .



###  http://purl.org/mp/quotedBy

:quotedBy rdf:type owl:ObjectProperty ;
          
          rdfs:subPropertyOf owl:topObjectProperty .



###  http://purl.org/mp/quotes

:quotes rdf:type owl:ObjectProperty ;
        
        owl:inverseOf :quotedBy ;
        
        rdfs:subPropertyOf owl:topObjectProperty .



###  http://purl.org/mp/representedBy

:representedBy rdf:type owl:ObjectProperty ;
               
               rdfs:domain :Artifact ;
               
               rdfs:range :Representation ;
               
               owl:inverseOf :represents ;
               
               rdfs:subPropertyOf <http://www.w3.org/ns/prov#hasDerivation> .



###  http://purl.org/mp/represents

:represents rdf:type owl:ObjectProperty ;
            
            rdfs:range :Artifact ;
            
            rdfs:domain :Representation ;
            
            rdfs:subPropertyOf :derivedFrom .



###  http://purl.org/mp/supportChallengedBy

:supportChallengedBy rdf:type owl:ObjectProperty ;
                     
                     owl:inverseOf :challengesSupportFor ;
                     
                     rdfs:subPropertyOf owl:topObjectProperty .



###  http://purl.org/mp/supportGraphFor

:supportGraphFor rdf:type owl:ObjectProperty ;
                 
                 owl:inverseOf :hasSupportGraph ;
                 
                 rdfs:subPropertyOf :supports .



###  http://purl.org/mp/supportedBy

:supportedBy rdf:type owl:ObjectProperty ,
                      owl:TransitiveProperty ;
             
             rdfs:range :Representation ;
             
             rdfs:domain :Representation ;
             
             owl:inverseOf :supports ;
             
             rdfs:subPropertyOf owl:topObjectProperty .



###  http://purl.org/mp/supports

:supports rdf:type owl:ObjectProperty ,
                   owl:TransitiveProperty ;
          
          rdfs:domain :Representation ;
          
          rdfs:range :Representation ;
          
          rdfs:subPropertyOf owl:topObjectProperty .



###  http://www.w3.org/ns/prov#hasDerivation

<http://www.w3.org/ns/prov#hasDerivation> rdf:type owl:ObjectProperty .





#################################################################
#
#    Classes
#
#################################################################


###  http://purl.org/mp/Artifact

:Artifact rdf:type owl:Class ;
          
          rdfs:comment "Artifact ::= a constructed or acquired material object, process or energy flow." .



###  http://purl.org/mp/Claim

:Claim rdf:type owl:Class ;
       
       owl:equivalentClass [ rdf:type owl:Class ;
                             owl:intersectionOf ( :Statement
                                                  [ rdf:type owl:Restriction ;
                                                    owl:onProperty :arguedBy ;
                                                    owl:someValuesFrom :Micropublication
                                                  ]
                                                )
                           ] ;
       
       rdfs:comment "Claim ::= the principal Statement argued for by a Micropublication" .



###  http://purl.org/mp/Data

:Data rdf:type owl:Class ;
      
      rdfs:subClassOf :ScientificEvidence ;
      
      rdfs:comment "Data ::= a series of Representations of the results of observation and/or experiment;" .



###  http://purl.org/mp/Holotype

:Holotype rdf:type owl:Class ;
          
          owl:equivalentClass [ rdf:type owl:Class ;
                                owl:intersectionOf ( :Statement
                                                     [ rdf:type owl:Restriction ;
                                                       owl:onProperty :holotypeFor ;
                                                       owl:someValuesFrom :Statement
                                                     ]
                                                   )
                              ] ;
          
          rdfs:subClassOf :Statement ;
          
          rdfs:comment "A Holotype is a member of a set of homologous Statements (a HomologyGroup), chosen as its representative for convenience.  If S is an HomologyGroup and si âˆˆ S is related to S by the holotypeFor relation, then si is the Holotype for S.  There may be only one Holotype per HomologyGroup." .



###  http://purl.org/mp/HomologyGroup

:HomologyGroup rdf:type owl:Class ;
               
               owl:equivalentClass [ rdf:type owl:Class ;
                                     owl:intersectionOf ( :Statement
                                                          [ rdf:type owl:Restriction ;
                                                            owl:onProperty :homologousTo ;
                                                            owl:someValuesFrom :Statement
                                                          ]
                                                        )
                                   ] ;
               
               rdfs:subClassOf :Statement .



###  http://purl.org/mp/Material

:Material rdf:type owl:Class ;
          
          rdfs:subClassOf :Method ;
          
          rdfs:comment "Material ::= part of a Method, describing physical reagents, tools, instruments, subjects, software, etc., used by the Actors in obtaining Data;" .



###  http://purl.org/mp/Method

:Method rdf:type owl:Class ;
        
        rdfs:subClassOf :ScientificEvidence ;
        
        rdfs:comment "Method := an account of a procedure for employing Artifacts to obtain a set of Data, including instructions for reproducibly making or obtaining the necessary Artiacts;" .



###  http://purl.org/mp/Micropublication

:Micropublication rdf:type owl:Class ;
                  
                  rdfs:subClassOf :Representation .



###  http://purl.org/mp/Procedure

:Procedure rdf:type owl:Class ;
           
           rdfs:subClassOf :Method ;
           
           rdfs:comment "Procedure ::= a type of Method, describing a series of actions, taken to obtain Data;" .



###  http://purl.org/mp/Provenance

:Provenance rdf:type owl:Class ;
            
            rdfs:subClassOf <http://www.w3.org/2004/03/trix/rdfg-1/Graph> .



###  http://purl.org/mp/QualifiedStatement

:QualifiedStatement rdf:type owl:Class ;
                    
                    owl:equivalentClass [ rdf:type owl:Class ;
                                          owl:intersectionOf ( :Statement
                                                               [ rdf:type owl:Restriction ;
                                                                 owl:onProperty :qualifiedBy ;
                                                                 owl:someValuesFrom :Qualifier
                                                               ]
                                                             )
                                        ] ;
                    
                    rdfs:subClassOf :Statement .



###  http://purl.org/mp/Qualifier

:Qualifier rdf:type owl:Class ;
           
           rdfs:subClassOf :Sentence ;
           
           rdfs:comment "Qualifier ::= a Sentence which modifies the epistemic strength, scope, or applicability of a Statement; may be a phrase or  even a single word." .



###  http://purl.org/mp/Reference

:Reference rdf:type owl:Class ;
           
           rdfs:subClassOf :Qualifier ;
           
           rdfs:comment "Reference ::= a Sentence purporting to indicate a certain document; deployed as to characterize a Statement, indicating that support for the Statement may be found in the referenced document." .



###  http://purl.org/mp/Representation

:Representation rdf:type owl:Class ;
                
                rdfs:subClassOf :Artifact ;
                
                rdfs:comment "Representation := a finite non-null sequence of digital Artifacts representing material objects, events, processes, states of affairs, and/or symbols; " .



###  http://purl.org/mp/ScientificEvidence

:ScientificEvidence rdf:type owl:Class ;
                    
                    rdfs:subClassOf :Representation ;
                    
                    rdfs:comment "ScientificEvidence ::= one or more sets of Data supported by re-usable Methods showing how the Data were obtained;" .



###  http://purl.org/mp/SemanticQualifier

:SemanticQualifier rdf:type owl:Class ;
                   
                   rdfs:subClassOf :Qualifier ;
                   
                   rdfs:comment "SemanticQualifier ::= a Qualifier which is a member of a defined formal Vocabulary." .



###  http://purl.org/mp/Sentence

:Sentence rdf:type owl:Class ;
          
          rdfs:subClassOf :Representation ;
          
          rdfs:comment "Sentence ::= meaningful symbolic Representation, well-formed in its domain." .



###  http://purl.org/mp/Statement

:Statement rdf:type owl:Class ;
           
           rdfs:subClassOf :Sentence ;
           
           rdfs:comment "Statement ::= a declarative Sentence; " .



###  http://purl.org/mp/SupportGraph

:SupportGraph rdf:type owl:Class ;
              
              owl:equivalentClass [ rdf:type owl:Class ;
                                    owl:intersectionOf ( :Representation
                                                         [ rdf:type owl:Class ;
                                                           owl:intersectionOf ( [ rdf:type owl:Class ;
                                                                                  owl:unionOf ( [ rdf:type owl:Restriction ;
                                                                                                  owl:onProperty :asserts ;
                                                                                                  owl:someValuesFrom :Representation
                                                                                                ]
                                                                                                [ rdf:type owl:Restriction ;
                                                                                                  owl:onProperty :quotes ;
                                                                                                  owl:someValuesFrom :Representation
                                                                                                ]
                                                                                              )
                                                                                ]
                                                                                [ rdf:type owl:Restriction ;
                                                                                  owl:onProperty :supportedBy ;
                                                                                  owl:someValuesFrom :Representation
                                                                                ]
                                                                              )
                                                         ]
                                                       )
                                  ] ;
              
              rdfs:subClassOf :Representation ,
                              <http://www.w3.org/2004/03/trix/rdfg-1/Graph> .



###  http://www.w3.org/2002/07/owl#Thing

owl:Thing rdf:type owl:Class .



###  http://www.w3.org/2004/03/trix/rdfg-1/Graph

<http://www.w3.org/2004/03/trix/rdfg-1/Graph> rdf:type owl:Class .





